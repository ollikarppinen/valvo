div.page-break {
  page-break-before: always;
}

.candidate-list {
  float:left;
  height:100%;
  width:49%;
  border-right: dashed 1px;
}

.voting-receipt {
  float:right;
  height:100%;
  width:49%;
  text-align: center;

}

.circle {
  margin: 200px 0 0 30px;
  width: 300px;
  height: 300px;
  border: 2px solid;
  -moz-border-radius: 50%;
  -webkit-border-radius: 50%;
  border-radius: 50%;

  .label {
    margin-top: 45%;
    margin-left: 20%;
    text-align: left;
  }
}


.qr {
  table {
    border-width: 0;
    border-style: none;
    border-color: #0000ff;
    border-collapse: collapse;
  }

  td {
    border-left: solid 10px #000;
    padding: 0;
    margin: 0;
    width: 0px;
    height: 10px;
  }

  td.black { border-color: #000; }
  td.white { border-color: #fff; }
}
